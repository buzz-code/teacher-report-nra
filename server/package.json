  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d shared/config/migration.config.ts",
    "typeorm:cli-js": "typeorm -d dist/shared/config/migration.config.js",
    "typeorm:cli-no-d": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm:cli -- migration:run",
    "typeorm:run:js": "npm run typeorm:cli-js -- migration:run",
    "typeorm:generate": "npm run typeorm:cli -- migration:generate",
    "typeorm:create": "npm run typeorm:cli-no-d -- migration:create",
    "typeorm:revert": "npm run typeorm:cli -- migration:revert",
    "clean-migrations": "node helpers/clean-migrations"
  },
